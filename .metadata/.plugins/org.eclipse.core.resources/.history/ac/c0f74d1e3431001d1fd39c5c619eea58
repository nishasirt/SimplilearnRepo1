package com.learn.revisionmethod;

public class LeapYear {
	
	
	void leapYear(int year)
	{
		if(year%4==0)
		{
			if(year%100==0)
			{
				
				if(year%400==0)
				{
					System.out.println("Year is leap year");
				}
				
			}
			
		}
		
		
		
		
		
	}
	
	public static void main(String[] args) {
		
		
		
		
		
		  
		
		
	}

}

package com.learning.selftut;

public class EnterPriseCustomer extends Customer{
	
	int custId;
	String name;
	String type;
	int amt;
	
	
	 public EnterPriseCustomer(int custId, String name, String type, int amt) {
		super(custId, name, type);
		
		this.custId=custId;
		this.name=name;
		this.amt=amt;
		this.type=type;
		
		
		// TODO Auto-generated constructor stub
	}

	@Override
	public double getAmount(double interest, int year) {
        double simple_interest=this.amt*interest*year;
        
        double amout=simple_interest+this.amt;
		
		return 0;
	}

}

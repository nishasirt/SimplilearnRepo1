package com.learn.revisionmethod;

public class Assignment5 {

	int sumDigit(int no) {

		String s = String.valueOf(no);

		char[] c;

		int sum = 0;

		for (int i = 0; i < s.length(); i++) {

			sum = sum + (s.charAt(i));

		}
		System.out.println("sum of " + no + " is : " + sum);

		return 0;
	}

	public static void main(String[] args) {

	}

}

package com.learning.selftut;

public class StudentRecord {
	
	public static void main(String[] args) {
		
		Student[] st=new Student[5];
		
		st[0]=new Student("Nisha", 11);
	}

}

class Student{
	
	
	
	String name;
	int rollNo;
	public Student(String name, int rollNo) {
		super();
		this.name = name;
		this.rollNo = rollNo;
	}
	
	
	
	
	
	
}
